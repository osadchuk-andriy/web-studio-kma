@model IList<web.model.User>
@{
    ViewBag.Title = "Index";
}

<h2>Stuff</h2>
@if (Model != null)
{
<table class="table">
    <thead>
        <tr>
            <th>id</th>
            <th>nick</th>
            <th>projects</th>
            <th>actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var user in Model)
        { 
        <tr>
            <td>@user.Id</td>
            <td>@user.Nick</td>
            <td>
                @foreach(var project in user.Projects)
                {
                    <div>@project.Project.Name <span>status=@project.Project.Status</span></div>
                }
            </td>
        </tr>
        }
    </tbody>
</table>
}

